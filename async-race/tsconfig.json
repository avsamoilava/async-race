{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["es2021", "dom"],
    "module": "ES6",
    "outDir": "./dist/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
